# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase project URL
VITE_SUPABASE_URL=your-supabase-url-here

# Your Supabase anon/public key
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Your Supabase service role key (for admin operations)
VITE_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Note: The environment variables are set in Netlify's dashboard under Site settings > Environment variables

# Printify API Configuration
VITE_PRINTIFY_API_TOKEN=your_printify_api_token_here
VITE_PRINTIFY_SHOP_ID=your_shop_id_here

# Optional: Set to 'development' or 'production'
VITE_APP_ENV=development

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here